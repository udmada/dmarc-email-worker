{
  "name": "dmarc-reports-email-worker",
  "version": "0.1.0",
  "scripts": {
    "deploy": "pnpm exec wrangler deploy",
    "format": "pnpm exec oxfmt --no-error-on-unmatched-pattern",
    "lint": "pnpm exec oxlint --type-aware --fix --fix-suggestions",
    "pretty": "pnpm exec oxfmt --check --no-error-on-unmatched-pattern",
    "start": "pnpm exec wrangler dev",
    "test": "pnpm exec oxfmt --check && pnpm exec oxlint --type-aware"
  },
  "dependencies": {
    "fast-xml-parser": "^5.3.5",
    "mime-db": "^1.54.0",
    "mimetext": "^3.0.28",
    "pako": "^2.1.0",
    "postal-mime": "^2.7.3",
    "postgres": "^3.4.8",
    "unzipit": "^1.4.3"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "0.12.8",
    "@cloudflare/workers-types": "^4.20260210.0",
    "@types/mime-db": "^1.43.6",
    "@types/mime-types": "^3.0.1",
    "@types/pako": "^2.0.4",
    "@vitest/ui": "3.2.4",
    "oxfmt": "latest",
    "oxlint": "latest",
    "oxlint-tsgolint": "^0.11.5",
    "typescript": "^5.9.3",
    "vitest": "3.2.4",
    "wrangler": "^4.64.0"
  }
}
